<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="po.JitaGroupPo" table="jita_group_user" lazy="false">
        <id name="id" type="long" column="id" length="20">
            <generator class="assigned"/>
        </id>
        <property name="groupName" type="java.lang.String" column="group_name"/>
        <property name="userId" type="long" column="user_id"/>
        <property name="showType" type="integer" column="show_type"/>
        <property name="itemList" type="java.lang.String" column="item_list"/>
        <property name="needCount" type="boolean" column="need_count"/>
        <property name="createDate" type="java.util.Date" column="create_date"/>
    </class>
</hibernate-mapping>