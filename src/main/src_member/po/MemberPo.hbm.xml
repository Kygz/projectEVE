<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="po.MemberPo" table="member" lazy="false">
        <id name="member_id" type="long" column="member_id" length="20">
            <generator class="assigned"/>
        </id>
        <property name="member_nickname" type="string" column="member_nickname" length="255" />
        <property name="member_name" type="string" column="member_name" length="255" />
        <property name="member_password" type="string" column="member_password" length="255" />
        <property name="member_qq" type="string" column="member_qq" length="255" />
        <property name="member_email" type="string" column="member_email" length="255" />
        <property name="member_skin" type="string" column="member_skin" length="255" />
    </class>
</hibernate-mapping>
