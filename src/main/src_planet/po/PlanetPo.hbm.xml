<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="po.PlanetPo" table="planet" lazy="false">
		<id name="planet_id" type="long" column="planet_id" length="20">
			<generator class="assigned"/>
		</id>
		<!-- 氧 电解物 水 稀有金属 有毒金属 反应金属 手性结构 -->
		<property name="planet_user_id" type="java.lang.String" column="planet_user_id"/>
		<property name="planet_user_name" type="java.lang.String" column="planet_user_name"/>
		<property name="planet_update_date" type="java.util.Date" column="planet_upload_date"/>
		<property name="planet_star1" type="long" column="planet_star_1"/>
		<property name="planet_star2" type="long" column="planet_star_2"/>
		<property name="planet_star3" type="long" column="planet_star_3"/>
		<property name="planet_star4" type="long" column="planet_star_4"/>
		<property name="planet_star5" type="long" column="planet_star_5"/>
		<property name="planet_star6" type="long" column="planet_star_6"/>
		<property name="planet_star7" type="long" column="planet_star_7"/>
		<property name="planet_star1_price" type="double" column="planet_star_1_price"/>
		<property name="planet_star2_price" type="double" column="planet_star_2_price"/>
		<property name="planet_star3_price" type="double" column="planet_star_3_price"/>
		<property name="planet_star4_price" type="double" column="planet_star_4_price"/>
		<property name="planet_star5_price" type="double" column="planet_star_5_price"/>
		<property name="planet_star6_price" type="double" column="planet_star_6_price"/>
		<property name="planet_star7_price" type="double" column="planet_star_7_price"/>
	</class>
</hibernate-mapping>